{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceRoot}",
        "${workspaceRoot}/test/extension-test-workspace.code-workspace"
      ],
      "sourceMaps": true,
      "outFiles": ["${workspaceRoot}/out/**/*.js"],
      "preLaunchTask": "npm: watch"
    }
  ]
}
